<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts">
	<excludes>
		<pattern>/css*</pattern>
		<pattern>/easyui*</pattern>
		<pattern>/images*</pattern>
		<pattern>/img*</pattern>
		<pattern>/js/*</pattern>
		<pattern>/staticjs/*</pattern>
		<pattern>/jslib/*</pattern>
		<pattern>/searchbox_files/*</pattern>
		<pattern>/style/*</pattern>
		<pattern>/themes/*</pattern>
	</excludes>
	<decorator name="default" page="default.jsp"> 
	     <!--首页 -->
		<pattern>/homePage/index.action</pattern> 
	</decorator> 
	
	<decorator name="common" page="default_new.jsp">
	     <!--样本处理 -->
	     <pattern>/experiment/index.action</pattern> 
	     <pattern>/experiment/sampleHander.action</pattern> 
	     <pattern>/experiment/jingjian.action</pattern> 
	     <pattern>/experiment/pingban.action</pattern> 
	     <pattern>/experiment/jianding.action</pattern> 
	     
	     
	     <pattern>/sampleInput/sampleType/index.action</pattern> 
	     <pattern>/sampleInput/sampleScan/scanIndex.action</pattern> 
	     <pattern>/analysisAction/index.action</pattern> 
	</decorator>
	
</decorators>
